{
  "name": "@vmo11/eslint-config-vmo",
  "version": "2.0.0",
  "description": "ESLint + Prettier",
  "main": "index.js",
  "files": [
    "lib",
    "base.js",
    "index.js",
    "jest.js",
    "base-js.js",
    "react-js.js",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "release": "npm run lint:fix && git tag v$npm_package_version && git push --tags",
    "commit": "git cz"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "^12.3.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
