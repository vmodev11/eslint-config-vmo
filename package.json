    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "eslint": "^7.29.0",
    "prettier": "^2.3.1"
  },
  "keywords": [
    "config",
    "eslint",
    "javascript",
    "typescript",
    "lint",
    "prettier"
  ],
  "author": "Huy Ha <huy.ha@vmodev.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmodev11/eslint-config-vmo.git"
  },
  "bugs": {
    "url": "https://github.com/vmodev11/eslint-config-vmo/issues"
  },
  "homepage": "https://github.com/vmodev11/eslint-config-vmo#readme",
  "devDependencies": {
    "@vmo11/cz-jira-smart-commit": "^3.0.1",
    "commitizen": "^4.2.4",
    "eslint": "^7.29.0",
    "prettier": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^4.6.0"
